    <script type="text/javascript">
        $(document).ready(function () {
            // navigation click actions
            $('.scroll-link').on('click', function (event) {
                event.preventDefault();
                var sectionID = $(this).attr("data-id");
                scrollToID('#' + sectionID, 750);
            });

            // navigation border add/remove

            $('.navbar-nav>li>a').click(function () {
                $('.navbar-nav>li>a').removeClass('current');
                $(this).addClass('current');
            });
            $('a.navbar-brand').click(function () {
                $('.navbar-nav>li>a').removeClass("current");
            });

            // scroll to top action
            $('.scroll-top').on('click', function (event) {
                event.preventDefault();
                $('html, body').animate({ scrollTop: 0 }, 'slow');
            });
            // mobile nav toggle
            $('#nav-toggle').on('click', function (event) {
                event.preventDefault();
                $('#main-nav').toggleClass("open");
            });


            // Live chat on click

            $('.livechat').on('click', function () {
                //const SRC = 'https://chatbot.ptcl.com.pk/customer-gadget/init_widget.js'; //document.getElementById("EF_init_widget").src;
                //const HostName = "https://chatbot.ptcl.com.pk/web/index.html"; //SRC.slice(0,SRC.indexOf("init_widget.js"));
                const SRC = '/livechatnew/init_widget.js';
                const HostName = "/livechatnew/prechat.html";
                var ifrm = document.createElement('iframe');
                ifrm.setAttribute('id', 'init_widget');
                ifrm.setAttribute('width', '0');
                ifrm.setAttribute('height', '0');
                ifrm.setAttribute('src', HostName);
                ifrm.style.border = '0';
                ifrm.style.float = 'right';
                ifrm.style.position = 'fixed';
                ifrm.style.bottom = '0';
                ifrm.style.right = '0';
                ifrm.style.background = 'rgba(0,0,0,.01)';
                ifrm.style.zIndex = 30;
                document.body.appendChild(ifrm);

                window.addEventListener("message", function (event) {
                    if (event.data.type !== "setDimensions")
                        return;
                    this.console.log(event);
                    let iframeObj = document.getElementById("init_widget");
                    iframeObj.width = event.data.width;
                    iframeObj.height = event.data.height;
                    iframeObj.style.bottom = event.data.bottom;

                }, false);

            });

            // Stop jumping to top on click

            $('a.livechat, a.pop-button').click(function (e) {
                e.preventDefault();
            });


        });
        // scroll function
        function scrollToID(id, speed) {
            var offSet = 50;
            var targetOffset = $(id).offset().top - offSet;
            var mainNav = $('#main-nav');
            $('html,body').animate({ scrollTop: targetOffset }, speed);
            if (mainNav.hasClass("open")) {
                mainNav.css("height", "1px").removeClass("in").addClass("collapse");
                mainNav.removeClass("open");
            }
        }
        if (typeof console === "undefined") {
            console = {
                log: function () { }
            };
        }

        // The visible amount of px
        // of any element currently in viewport.

        ; (function ($, win) {
            $.fn.inViewport = function (cb) {
                return this.each(function (i, el) {
                    function visPx() {
                        var H = $(this).height(),
                            r = el.getBoundingClientRect(), t = r.top, b = r.bottom;
                        return cb.call(el, Math.max(0, t > 0 ? H - t : (b < H ? b : H)));
                    } visPx();
                    $(win).on("resize scroll", visPx);
                });
            };
        }(jQuery, window));


        // trigger animations

        $(".navbar-brand").inViewport(function (px) {
            if (px) $(this).addClass("scale-up-center");
